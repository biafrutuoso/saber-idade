algoritmo "marcia01"
var
   ano_atual, ano_nasc, idade: inteiro
inicio
      Escreva ("Em que ano estamos? ")
      Leia (ano_atual)
      Escreva ("Em que ano a pessoa nasceu? ")
      Leia (ano_nasc)
      idade <- ano_atual - ano_nasc
      Escreva ("A idade da pessoa é:", idade)
fimalgoritmo